#ifdef USE_INSN_RET_NUM
CONSTFUNC(MAYBE_UNUSED(static int insn_ret_num(VALUE insn)));

int
insn_ret_num(VALUE i)
{
    static const char t[] = {
% RubyVM::Instructions.each_slice 16 do |a|
      <%= a.map {|i| i.rets.size }.join(', ') -%>,
% end
    };

    return t[i];
}
#endif
