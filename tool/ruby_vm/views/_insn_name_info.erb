CONSTFUNC(MAYBE_UNUSED(static const char *insn_name(VALUE insn)));

const char *
insn_name(VALUE i)
{
    static const char *const t[] = {
% RubyVM::Instructions.each do |i|
        <%=cstr i.name %>,
% end
    };

    return t[i];
}
